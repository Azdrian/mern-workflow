{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "no-console": "off", 
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
    "consistent-return": "off"
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}